<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="style_h3">
        <item name="android:textSize">24sp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">#303437</item>
        <item name="android:fontFamily">
            @font/lato
        </item>
    </style>

    <style name="style_h5">
        <item name="android:textSize">
            16sp
        </item>
        <item name="android:textColor">
            #000000
        </item>
        <item name="android:fontFamily">
            @font/lato
        </item>
        <item name="android:lineHeight">
            18sp
        </item>
    </style>

    <style name="style_h6">
        <item name="android:textSize">
            12sp
        </item>
        <item name="android:textColor">
            #202325
        </item>
        <item name="android:fontFamily">
            @font/lato
        </item>
        <item name="android:lineHeight">
            18sp
        </item>
    </style>


    <style name="body_primary">

        <item name="android:textSize">
            14sp
        </item>

        <item name="android:textColor">
            #979C9E
        </item>
        <item name="android:fontFamily">
            @font/lato
        </item>

    </style>

    <style name="body_primary_2">

        <item name="android:textSize">
            12sp
        </item>
        <item name="android:fontFamily">
            @font/lato
        </item>

    </style>

    <style name="DialogStyle" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:colorBackground">@android:color/transparent</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>

    <style name="LeadMgmtMaterialButtonStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="backgroundTint">@color/button_state_color</item>
    </style>


</resources>