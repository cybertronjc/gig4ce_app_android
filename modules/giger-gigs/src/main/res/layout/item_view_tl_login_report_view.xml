        <include
            android:id="@+id/in_training_layout"
            layout="@layout/item_view_tl_login_report_item_view"/>

        <include
            android:id="@+id/new_login_layout"
            layout="@layout/item_view_tl_login_report_item_view"/>

        <include
            android:id="@+id/open_positions_layout"
            layout="@layout/item_view_tl_login_report_item_view"/>

        <include
