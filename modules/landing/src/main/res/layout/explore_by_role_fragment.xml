        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
          app:srcCompat="@drawable/ic_heart_gray"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/role_main_view"
        app:layout_constraintStart_toStartOf="@+id/status_bar"
        app:layout_constraintTop_toTopOf="@+id/status_bar" />

    <TextView
        android:id="@+id/interest_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add_as_interest"
        android:layout_marginLeft="8dp"
        android:fontFamily="@font/lato"
        android:letterSpacing="0"
        app:layout_constraintBottom_toBottomOf="@+id/role_main_view"
        app:layout_constraintStart_toEndOf="@+id/heart_icon"
        app:layout_constraintTop_toTopOf="@+id/status_bar" />

    <TextView
