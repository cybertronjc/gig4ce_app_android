<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gigforce.landing_screen.landingscreen.explorebyrole.ExploreByRoleFragment">

    <androidx.cardview.widget.CardView
        android:id="@+id/appbar_role"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/backpress_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
          app:srcCompat="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintBottom_toBottomOf="@+id/appbar_role"
        app:layout_constraintStart_toStartOf="@+id/appbar_role"
        app:layout_constraintTop_toTopOf="@+id/appbar_role"
        android:layout_marginLeft="16dp"
        android:elevation="5dp"/>

    <TextView
        android:id="@+id/textView128"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="#080808"
        android:text="@string/explore_by_role"
        android:fontFamily="@font/lato"
        android:letterSpacing="0"
        app:layout_constraintBottom_toBottomOf="@+id/appbar_role"
        app:layout_constraintStart_toEndOf="@+id/backpress_icon"
        app:layout_constraintTop_toTopOf="@+id/appbar_role"
        android:elevation="5dp"
        android:layout_marginLeft="16dp"/>

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
          app:srcCompat="@drawable/ic_landinghome_search"
        app:layout_constraintBottom_toBottomOf="@+id/appbar_role"
        app:layout_constraintEnd_toEndOf="@+id/appbar_role"
        app:layout_constraintTop_toTopOf="@+id/appbar_role"
        android:layout_marginRight="16dp"
        android:elevation="5dp"/>


    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="3dp"
        android:background="@drawable/rect_button_outline_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appbar_role" />

    <View
        android:id="@+id/textView129"
        android:layout_width="3dp"
        android:layout_height="24dp"
        android:background="#cdcdcd"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/appbar_role" />

    <TextView
        android:id="@+id/textView130"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sort_by_popular"
        android:layout_marginLeft="24dp"
        android:textSize="14sp"
        android:textColor="#000000"
        android:letterSpacing="0"
        android:fontFamily="@font/lato"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/appbar_role" />

    <TextView
        android:id="@+id/textView131"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/filter_role"
        android:textSize="14sp"
        android:textColor="#000000"
        android:letterSpacing="0"
        android:layout_marginLeft="24dp"
        android:fontFamily="@font/lato"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintStart_toEndOf="@+id/textView129"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <View
        android:id="@+id/role_main_view"
        android:layout_width="match_parent"
        android:layout_height="144dp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/view2"/>

    <ImageView
        android:id="@+id/role_logo"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toTopOf="@+id/status_bar"
        app:layout_constraintStart_toStartOf="@+id/role_main_view"
        app:layout_constraintTop_toTopOf="@+id/role_main_view"
          app:srcCompat="@drawable/ic_profile_pic_2" />

    <TextView
