<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="169dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/textView124"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/blackLightFull" />

    <com.gigforce.core.recyclerView.CoreRecyclerView
        android:id="@+id/feature_item_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/textView124"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginLeft="@dimen/size16"
        android:orientation="vertical"
        android:scrollbars="none"/>

    <View
        android:id="@+id/grayline3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/feature_item_rv"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/blackLightFull" />

</androidx.constraintlayout.widget.ConstraintLayout>
