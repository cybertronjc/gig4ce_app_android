<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape
            android:shape="rectangle">
            <solid android:color="#EFEEF3" />
            <corners android:topLeftRadius="@dimen/size_12"
                android:topRightRadius="@dimen/size_12"
                android:bottomRightRadius="@dimen/size_12"
                android:bottomLeftRadius="0dp"/>
        </shape>
    </item>
    <item android:right="1dp" android:left="1dp" android:bottom="1dp" android:top="1dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:topLeftRadius="@dimen/size_12"
                android:topRightRadius="@dimen/size_12"
                android:bottomRightRadius="@dimen/size_12"
                android:bottomLeftRadius="0dp"/>
        </shape>
    </item>
</layer-list>