<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopProfileBar">
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="user_name" format="string"/>
        <attr name="image_name" format="string"/>
    </declare-styleable>

    <declare-styleable name="ProfileCardBackground">
        <attr name="card_title" format="string" />
        <attr name="card_content" format="string" />
        <attr name="card_bottom" format="string" />
    </declare-styleable>

    <declare-styleable name="CardRow">
        <attr name="row_content" format="string" />
        <attr name="location" format="string"/>
    </declare-styleable>

    <declare-styleable name="OBToggleButtonItem">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="OBTextViewItem">
        <attr name="textview" format="string" />
    </declare-styleable>

    <declare-styleable name="ExtendedBottomSheetBehavior">
        <attr name="hideable" format="boolean"/>
        <attr name="peekHeight" format="dimension"/>
        <attr name="skipCollapsed" format="boolean"/>
        <attr name="halfOffset" format="dimension"/>
        <attr name="allowDragging" format="boolean"/>
    </declare-styleable>
</resources>
