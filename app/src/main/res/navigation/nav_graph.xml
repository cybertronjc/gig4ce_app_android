<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/aboutExpandedFragment"
        android:name="com.gigforce.app.modules.profile.AboutExpandedFragment"
        android:label="AboutExpandedFragment"
        tools:layout="@layout/fragment_profile_about_expanded">
        <action
            android:id="@+id/action_aboutExpandedFragment_to_addContactBottomSheetFragment"
            app:destination="@id/addContactBottomSheetFragment" />
        <action
            android:id="@+id/action_aboutExpandedFragment_to_addLanguageBottomSheetFragment"
            app:destination="@id/addLanguageBottomSheetFragment" />
    </fragment>
    <dialog
        android:id="@+id/addContactBottomSheetFragment"
        android:name="com.gigforce.app.modules.profile.AddContactBottomSheetFragment"
        android:label="AddContactBottomSheetFragment"
        tools:layout="@layout/add_contact_bottom_sheet" />
    <dialog
        android:id="@+id/addLanguageBottomSheetFragment"
        android:name="com.gigforce.app.modules.profile.AddLanguageBottomSheetFragment"
        android:label="AddLanguageBottomSheetFragment" />

    <fragment android:id="@+id/authFlowFragment"
        android:name="com.gigforce.app.modules.auth.AuthFlowFragment"
        tools:layout="@layout/fragment_auth_flow"/>

    <fragment android:id="@+id/settingFragment"
        android:name="com.gigforce.app.modules.preferences.SettingFragment"
        tools:layout="@layout/fragment_auth_flow"/>
    <fragment android:id="@+id/dayTimeFragment"
        android:name="com.gigforce.app.modules.preferences.datetime.DayTimeFragment"
        tools:layout="@layout/date_time_fragment"/>

    <fragment android:id="@+id/weekEndFragment"
        android:name="com.gigforce.app.modules.preferences.datetime.weekend.WeekEndFragment"
        tools:layout="@layout/week_end_fragment"/>

    <fragment android:id="@+id/weekDayFragment"
        android:name="com.gigforce.app.modules.preferences.datetime.weekday.WeekDayFragment"
        tools:layout="@layout/week_day_fragment"/>

    <fragment
        android:id="@+id/experienceExpandedFragment"
        android:name="com.gigforce.app.modules.profile.ExperienceExpandedFragment"
        android:label="ExperienceExpandedFragment"
        tools:layout="@layout/fragment_profile_experience_expanded">
        <action
            android:id="@+id/action_experienceExpandedFragment_to_editExperienceBottomSheet"
            app:destination="@id/editExperienceBottomSheet" />
        <action
            android:id="@+id/action_experienceExpandedFragment_to_addExperienceBottomSheet"
            app:destination="@id/addExperienceBottomSheet" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.gigforce.app.modules.home.HomeFragment"
        android:label="home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_homeScreenIcons"
            app:destination="@id/homeScreenIcons" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.gigforce.app.modules.profile.ProfileFragment"
        android:label="profile_fragment"
        tools:layout="@layout/fragment_profile_main_expanded">
        <action
            android:id="@+id/action_profileFragment_to_educationExpandedFragment"
            app:destination="@id/educationExpandedFragment" />
        <action
            android:id="@+id/action_profileFragment_to_aboutExpandedFragment"
            app:destination="@id/aboutExpandedFragment" />
        <action
            android:id="@+id/action_profileFragment_to_experienceExpandedFragment"
            app:destination="@id/experienceExpandedFragment" />

        <action
            android:id="@+id/action_profileFragment_to_experienceExpandedFragment2"
            app:destination="@id/experienceExpandedFragment" />
        <action
            android:id="@+id/action_profileFragment_to_addTagBottomSheet"
            app:destination="@id/addTagBottomSheet" />
    </fragment>
    <fragment
        android:id="@+id/chatsHomeFragment"
        android:name="com.gigforce.app.modules.chat.ChatsHomeFragment"
        android:label="chats_home_fragment"
        tools:layout="@layout/fragment_chats_home">
        <action
            android:id="@+id/action_chatsHomeFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.gigforce.app.modules.chat.ChatFragment"
        android:label="chat_fragment"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/educationExpandedFragment"
        android:name="com.gigforce.app.modules.profile.EducationExpandedFragment"
        android:label="EducationExpandedFragment"
        tools:layout="@layout/fragment_profile_education_expanded">
        <action
            android:id="@+id/action_educationExpandedFragment_to_addAchievementBottomSheetFragment"
            app:destination="@id/addAchievementBottomSheetFragment" />
        <action
            android:id="@+id/action_educationExpandedFragment_to_addEducationBottomSheetFragment"
            app:destination="@id/addEducationBottomSheetFragment" />
        <action
            android:id="@+id/action_educationExpandedFragment_to_addSkillBottomSheetFragment"
            app:destination="@id/addSkillBottomSheetFragment" />
        <action
            android:id="@+id/action_educationExpandedFragment_to_editEducationBottomSheet"
            app:destination="@id/editEducationBottomSheet" />
        <action
            android:id="@+id/action_educationExpandedFragment_to_editAchievementBottomSheet"
            app:destination="@id/editAchievementBottomSheet" />
        <action
            android:id="@+id/action_educationExpandedFragment_to_editSkillBottomSheet"
            app:destination="@id/editSkillBottomSheet" />
    </fragment>
    <dialog
        android:id="@+id/addSkillBottomSheetFragment"
        android:name="com.gigforce.app.modules.profile.AddSkillBottomSheetFragment"
        android:label="AddSkillBottomSheetFragment"
        tools:layout="@layout/add_skill_bottom_sheet" />
    <dialog
        android:id="@+id/addAchievementBottomSheetFragment"
        android:name="com.gigforce.app.modules.profile.AddAchievementBottomSheetFragment"
        android:label="AddAchievementBottomSheetFragment"
        tools:layout="@layout/add_achievement_bottom_sheet" />
    <dialog
        android:id="@+id/addEducationBottomSheetFragment"
        android:name="com.gigforce.app.modules.profile.AddEducationBottomSheetFragment"
        android:label="AddEducationBottomSheetFragment"
        tools:layout="@layout/add_education_bottom_sheet" />

    <fragment
        android:id="@+id/createInitProfile"
        android:name="com.gigforce.app.modules.onboarding.CreateInitProfile"
        android:label="CreateInitProfile" />
    <fragment
        android:id="@+id/introSlidesFragment"
        android:name="com.gigforce.app.modules.onboarding.IntroSlidesFragment"
        android:label="fragment_intro_slides"
        tools:layout="@layout/fragment_intro_slides" />
    <fragment
        android:id="@+id/OBSlidesFragment"
        android:name="com.gigforce.app.modules.onboarding.OBSlidesFragment"
        android:label="OBSlidesFragment">
        <action
            android:id="@+id/action_OBSlidesFragment_to_createInitProfile"
            app:destination="@id/createInitProfile" />
    </fragment>
    <fragment
        android:id="@+id/languageSelectFragment"
        android:name="com.gigforce.app.utils.LanguageSelectFragment"
        android:label="LanguageSelectFragment" />
    <fragment
        android:id="@+id/videoResumeFragment"
        android:name="com.gigforce.app.modules.video_resume.VideoResumeFragment"
        android:label="fragment_video_resume"
        tools:layout="@layout/fragment_video_resume" />
    <fragment
        android:id="@+id/verifyOTP"
        android:name="com.gigforce.app.modules.auth.ui.main.VerifyOTP"
        android:label="VerifyOTP">
        <argument
            android:name="verificationId"
            android:defaultValue='""'
            app:argType="string" />
        <action
            android:id="@+id/action_verifyOTP_to_homeScreenIcons"
            app:destination="@id/homeScreenIcons" />
    </fragment>
    <fragment
        android:id="@+id/homeScreenFragment"
        android:name="com.gigforce.app.modules.homescreen.HomeScreenFragment"
        android:label="HomeScreenFragment">
        <action
            android:id="@+id/action_homeScreenFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeScreenIcons"
        android:name="com.gigforce.app.modules.homescreen.HomeScreenIcons"
        android:label="HomeScreenIcons">
        <action
            android:id="@+id/action_homeScreenIcons_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeScreenIcons_to_verification"
            app:destination="@id/verification" />
    </fragment>
    <dialog
        android:id="@+id/addTagBottomSheet"
        android:name="com.gigforce.app.modules.profile.AddTagBottomSheet"
        android:label="AddTagBottomSheet"
        tools:layout="@layout/add_tag_bottom_sheet" />
    <dialog
        android:id="@+id/addExperienceBottomSheet"
        android:name="com.gigforce.app.modules.profile.AddExperienceBottomSheet"
        android:label="AddExperienceBottomSheet" />
    <fragment
        android:id="@+id/verification"
        android:name="com.gigforce.app.modules.verification.Verification"
        android:label="Verification">
        <action
            android:id="@+id/action_verification_to_aadhaarUpload"
            app:destination="@id/aadhaarUpload" />
        <action
            android:id="@+id/action_verification_to_uploadDropDown"
            app:destination="@id/uploadDropDown" />
    </fragment>
    <fragment
        android:id="@+id/Login"
        android:name="com.gigforce.app.modules.auth.ui.main.Login"
        android:label="Login">
        <action
            android:id="@+id/action_login2_to_verifyOTP"
            app:destination="@id/verifyOTP" />
    </fragment>

    <fragment
        android:id="@+id/panUpload"
        android:name="com.gigforce.app.modules.verification.PanUpload"
        android:label="PanUpload">
        <action
            android:id="@+id/action_panUpload_to_verificationDone"
            app:destination="@id/verificationDone" />
    </fragment>
    <fragment
        android:id="@+id/aadhaarUpload"
        android:name="com.gigforce.app.modules.verification.AadhaarUpload"
        android:label="AadhaarUpload">
        <action
            android:id="@+id/action_aadhaarUpload_to_uploadDropDown"
            app:destination="@id/uploadDropDown" />
    </fragment>

    <dialog
        android:id="@+id/editEducationBottomSheet"
        android:name="com.gigforce.app.modules.profile.EditEducationBottomSheet"
        android:label="EditEducationBottomSheet">
        <argument
            android:name="array_location"
            app:argType="string" />
    </dialog>
    <dialog
        android:id="@+id/editAchievementBottomSheet"
        android:name="com.gigforce.app.modules.profile.EditAchievementBottomSheet"
        android:label="EditAchievementBottomSheet">
        <argument
            android:name="array_location"
            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/DLUpload"
        android:name="com.gigforce.app.modules.verification.DLUpload"
        android:label="DLUpload">
        <action
            android:id="@+id/action_DLUpload_to_bankUpload2"
            app:destination="@id/bankUpload2" />
    </fragment>
    <dialog
        android:id="@+id/editSkillBottomSheet"
        android:name="com.gigforce.app.modules.profile.EditSkillBottomSheet"
        android:label="EditSkillBottomSheet">
        <argument
            android:name="array_location"
            app:argType="string" />
    </dialog>
    <dialog
        android:id="@+id/editExperienceBottomSheet"
        android:name="com.gigforce.app.modules.profile.EditExperienceBottomSheet"
        android:label="EditExperienceBottomSheet">
        <argument
            android:name="array_location"
            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/verificationDone"
        android:name="com.gigforce.app.modules.verification.VerificationDone"
        android:label="VerificationDone" />
    <fragment
        android:id="@+id/bankUpload2"
        android:name="com.gigforce.app.modules.verification.BankUpload"
        android:label="BankUpload">
        <action
            android:id="@+id/action_bankUpload2_to_panUpload"
            app:destination="@id/panUpload" />
    </fragment>
    <fragment
        android:id="@+id/uploadDropDown"
        android:name="com.gigforce.app.modules.verification.UploadDropDown"
        android:label="UploadDropDown">
        <action
            android:id="@+id/action_uploadDropDown_to_bankUpload2"
            app:destination="@id/bankUpload2" />
    </fragment>
</navigation>

