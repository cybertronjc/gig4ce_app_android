<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/setting_bg"
    tools:context=".modules.preferences.earnings.EarningFragment">

    <View
        android:id="@+id/textView100"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/white"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:outlineSpotShadowColor="@color/black"
        android:src="@drawable/ic_arrow_back_black"
        app:layout_constraintBottom_toBottomOf="@id/textView100"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/textView100" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:fontFamily="@font/lato"
        android:text="@string/earning"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:letterSpacing="0"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@id/textView100"
        app:layout_constraintBottom_toBottomOf="@id/textView100" />

    <View
        android:id="@+id/preferredNoOfDays"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/white"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView100" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_calendar_month"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/preferredNoOfDays"
        app:layout_constraintStart_toStartOf="@+id/preferredNoOfDays"
        app:layout_constraintTop_toTopOf="@+id/preferredNoOfDays" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="19dp"
        android:letterSpacing="0"
        android:text="Preferred no. of days of work in a month"
        android:textColor="#000000"
        android:textSize="14sp"
        android:fontFamily="@font/lato"
        app:layout_constraintStart_toEndOf="@+id/imageView28"
        app:layout_constraintTop_toTopOf="@+id/imageView28" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00- 04 Days"
        android:textSize="10sp"
        android:textColor="#6d7278"
        android:letterSpacing="0"
        android:fontFamily="@font/lato"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <View
        android:id="@+id/textView102"
        android:layout_width="match_parent"
        android:layout_height="108dp"
        android:background="@color/white"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/preferredNoOfDays" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Daily  Goals"
        android:layout_marginLeft="67dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/lato"
        android:textSize="14sp"
        android:textColor="#000000"
        android:letterSpacing="-0.01"
        android:lineSpacingExtra="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView102" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rs0 - Rs2000"
        android:textSize="12sp"
        android:textColor="#979797"
        android:letterSpacing="0"
        android:lineSpacingExtra="10sp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView102" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Per day goal"
        android:layout_marginTop="8dp"
        android:textSize="12sp"
        android:textColor="#000000"
        android:letterSpacing="0"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <com.gigforce.app.core.components.SeekBarWithHint
        android:id="@+id/seekBarWithHint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="28dp"
        android:layout_marginTop="20dp"
        android:progressTint="@color/colorPrimary"
        android:thumbTint="@color/colorPrimary"
        android:min="0"
        android:max="2000"
        app:layout_constraintEnd_toEndOf="@+id/textView102"
        app:layout_constraintStart_toStartOf="@+id/textView102"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <com.gigforce.app.core.components.SeekBarDependentCanvas
        android:id="@+id/seekBarDependentCanvas2"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBarWithHint" />

    <View
        android:id="@+id/textView103"
        android:layout_width="match_parent"
        android:layout_height="108dp"
        android:background="@color/white"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView102" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="67dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/lato"
        android:letterSpacing="-0.01"
        android:lineSpacingExtra="8sp"
        android:text="Monthy Goals"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView103" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rs0 - Rs20000"
        android:textSize="12sp"
        android:textColor="#979797"
        android:letterSpacing="0"
        android:lineSpacingExtra="10sp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView103" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:letterSpacing="0"
        android:text="Per Month goal"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <com.gigforce.app.core.components.SeekBarWithHint
        android:id="@+id/seekBarWithHint1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="28dp"
        android:layout_marginTop="20dp"
        android:progressTint="@color/colorPrimary"
        android:thumbTint="@color/colorPrimary"
        android:min="0"
        android:max="2000"
        app:layout_constraintEnd_toEndOf="@+id/textView103"
        app:layout_constraintStart_toStartOf="@+id/textView103"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <com.gigforce.app.core.components.SeekBarDependentCanvas
        android:id="@+id/seekBarDependentCanvas3"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBarWithHint1" />

    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView103"
        android:layout_marginTop="16dp"
        android:background="@color/white">

    <View
        android:id="@+id/textView104"
        android:layout_width="match_parent"
        android:layout_height="61dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <CheckBox
        android:id="@+id/checkbox_monthly_constract"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Willing to do Monthly Contract"
        android:layout_marginLeft="36dp"
        android:paddingLeft="10dp"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:letterSpacing="0"
        android:theme="@style/Theme.checkbox"
        app:layout_constraintBottom_toBottomOf="@+id/textView104"
        app:layout_constraintStart_toStartOf="@+id/textView104"
        app:layout_constraintTop_toTopOf="@+id/textView104" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/monthly_expectation_constraintlayout"
            android:layout_width="match_parent"
            android:layout_height="79dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView104"
            android:visibility="gone">
        <TextView
            android:id="@+id/textView85"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Monthly Expectation"
            android:layout_marginLeft="46dp"
            android:textSize="14sp"
            android:textColor="#000000"
            android:letterSpacing="-0.01"
            android:lineSpacingExtra="8sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

            <com.gigforce.app.core.components.SeekBarWithHint
                android:id="@+id/seekBarWithHint2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="45dp"
                android:layout_marginRight="28dp"
                android:layout_marginTop="20dp"
                android:progressTint="@color/colorPrimary"
                android:thumbTint="@color/colorPrimary"
                android:min="0"
                android:max="20000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView85" />

            <com.gigforce.app.core.components.SeekBarDependentCanvas
                android:id="@+id/seekBarDependentCanvas4"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="22dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seekBarWithHint2" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>