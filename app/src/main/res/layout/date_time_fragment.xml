<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/setting_bg"
    tools:context=".modules.preferences.daytime.DayTimeFragment">


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginLeft="19dp"
        android:fontFamily="@font/lato_bold"
        android:text="Day and Time"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/textView47"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginTop="28dp"
        android:background="@color/shadow_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView46" />

    <View
        android:id="@+id/textView49"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/textView47"
        tools:layout_editor_absoluteX="-16dp" />

    <ImageView
        android:id="@+id/editCurrentLocation"
        android:layout_width="20dp"
        android:layout_height="21dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/textView49"
        app:layout_constraintTop_toTopOf="@+id/textView49"
        android:src="@drawable/ic_calendar_black" />

    <TextView
        android:id="@+id/textView50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Weekday"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/lato"
        app:layout_constraintStart_toStartOf="@+id/textView49"
        app:layout_constraintTop_toTopOf="@+id/textView49" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00-04days"
        android:textColor="@color/gray_color"
        android:textSize="10sp"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/lato"
        app:layout_constraintStart_toStartOf="@+id/textView49"
        app:layout_constraintTop_toBottomOf="@+id/textView50" />

    <Switch
        android:id="@+id/workFromHomeSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="24dp"
        android:theme="@style/Theme.switchButton"
        app:layout_constraintBottom_toBottomOf="@+id/textView49"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView49" />

    <View
        android:id="@+id/textView53"
        android:layout_width="3dp"
        android:layout_height="24dp"
        android:background="@color/gray_color"
        android:layout_marginRight="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView49"
        app:layout_constraintEnd_toStartOf="@+id/workFromHomeSwitch"
        app:layout_constraintTop_toBottomOf="@+id/textView47" />

    <View
        android:id="@+id/textView54"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/shadow_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView49" />

    <View
        android:id="@+id/textView55"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/textView54"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="20dp"
        android:layout_height="21dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_calendar_black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView55" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato"
        android:text="Weekends"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/textView55"
        app:layout_constraintTop_toTopOf="@+id/textView55" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato"
        android:text="No"
        android:textColor="@color/gray_color"
        android:textSize="10sp"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView55"
        app:layout_constraintTop_toBottomOf="@+id/textView56" />

    <Switch
        android:id="@+id/arroundCurrentAddSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="24dp"
        android:theme="@style/Theme.switchButton"
        app:layout_constraintBottom_toBottomOf="@+id/textView55"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView55" />

    <View
        android:id="@+id/textView58"
        android:layout_width="3dp"
        android:layout_height="24dp"
        android:background="@color/gray_color"
        android:layout_marginRight="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView55"
        app:layout_constraintEnd_toStartOf="@+id/arroundCurrentAddSwitch"
        app:layout_constraintTop_toTopOf="@+id/textView55" />
</androidx.constraintlayout.widget.ConstraintLayout>