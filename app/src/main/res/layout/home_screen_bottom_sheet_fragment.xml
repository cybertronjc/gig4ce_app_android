<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linear_layout"

    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical">

    <View
        android:id="@+id/v_hs1bs_partition"
        android:layout_width="64dp"
        android:layout_height="3dp"
        android:layout_gravity="center"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="@dimen/size_16"
        android:background="@android:color/darker_gray" />


    <LinearLayout
        android:id="@+id/kyc_video_resume"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="18dp"
        android:orientation="vertical"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="@drawable/kyc_rect_border"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                  app:srcCompat="@drawable/warning_icon_" />

            <TextView
                android:id="@+id/kyc_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp"
                android:layout_weight="1"
                android:letterSpacing="0"
                android:text="@string/kyc_verification_not_done"
                android:textColor="#4a4a4a"
                android:textSize="13sp" />

            <ImageView
                android:id="@+id/show_upcominggig_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="14dp"
                android:layout_marginRight="9dp"
                  app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/video_resume"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/kyc_rect_border"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                  app:srcCompat="@drawable/warning_icon_" />

            <TextView
                android:id="@+id/video_resume_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp"
                android:layout_weight="1"
                android:letterSpacing="0"
                android:text="@string/video_resume_pending"
                android:textColor="#4a4a4a"
                android:textSize="13sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="14dp"
                android:layout_marginRight="9dp"
                  app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24" />
        </LinearLayout>

    </LinearLayout>


    <TextView
        android:id="@+id/upcoming_gig_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:letterSpacing="0"
        android:text="@string/upcoming_gig_"
        android:textColor="#080808"
        android:textSize="16sp" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/upcoming_gig_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:nestedScrollingEnabled="false"
        android:paddingTop="14dp"
        android:paddingRight="20dp"
        android:paddingBottom="15dp"
        android:scrollbars="none" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:fontFamily="@font/lato_bold"
        android:letterSpacing="0"
        android:text="@string/features"
        android:textColor="#080808"
        android:textSize="18sp" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/feature_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="11.5dp"
        android:clipToPadding="false"
        android:nestedScrollingEnabled="false"
        android:paddingRight="20dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:layout_marginTop="9.5dp"
        android:background="#f1f1f1" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/ambassador_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_16"
        android:layout_marginTop="@dimen/size_22"
        android:layout_marginEnd="@dimen/size_16"
        android:layout_marginBottom="@dimen/size_16"
        app:cardBackgroundColor="@color/light_pink">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:padding="16dp">


            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="90dp"
                android:layout_height="105dp"
                  app:srcCompat="@drawable/banner_ambassador"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/join_as_amb_label" />


            <TextView
                android:id="@+id/join_as_amb_label"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:fontFamily="@font/lato"
                android:lineSpacingExtra="3sp"
                android:text="@string/ambassador_program"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView11"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/amb_subtitle_landing_screen"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:layout_marginTop="12dp"
                android:fontFamily="@font/lato"
                android:lineSpacingExtra="3sp"
                android:text="@string/more_you_create_profiles_more_you_earn"
                android:textColor="#d9000000"
                android:textSize="12sp"
                android:textStyle="normal"
                app:layout_constraintEnd_toEndOf="@+id/amb_join_open_btn"
                app:layout_constraintStart_toEndOf="@+id/imageView11"
                app:layout_constraintTop_toBottomOf="@id/join_as_amb_label" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/amb_join_open_btn"
                style="@style/Widget.MaterialComponents.Button.TextButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/lato"
                android:minHeight="@dimen/size_36"
                android:textStyle="bold"
                android:textSize="14sp"
                android:textColor="@color/lipstick"
                android:lineSpacingExtra="3sp"
                android:gravity="center_horizontal"
                android:text="@string/open"
                android:textAllCaps="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:background="@color/colorPrimary"
        android:visibility="gone">

        <ImageView
            android:id="@+id/imageView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="16dp"
              app:srcCompat="@drawable/ic_group_interest"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView109"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:fontFamily="@font/lato_bold"
            android:letterSpacing="0"
            android:text="@string/my_interest"
            android:textColor="@color/white"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView33"
            app:layout_constraintStart_toEndOf="@+id/imageView33"
            app:layout_constraintTop_toTopOf="@+id/imageView33" />

        <TextView
            android:id="@+id/textView113"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/lato"
            android:letterSpacing="0"
            android:text="@string/explore_gigs_start_earning"
            android:textColor="@color/white"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/textView109"
            app:layout_constraintTop_toBottomOf="@+id/textView109" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout
        android:id="@+id/rl_cient_activation_bs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_title_client_activation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/lato_bold"
            android:letterSpacing="0"
            android:text="@string/explore_gigs"
            android:textColor="#080808"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <FrameLayout
            android:id="@+id/client_activation_frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_title_client_activation"
            android:layout_marginTop="@dimen/size_16"

            >

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/client_activation_rv_bs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="15dp"
                android:clipToPadding="false"
                android:nestedScrollingEnabled="false"
                android:paddingLeft="@dimen/size_16"
                android:paddingRight="20dp"
                android:paddingBottom="@dimen/size_4"
                android:visibility="gone" />

            <ProgressBar
                android:id="@+id/client_activation_progress_bar_bs"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:visibility="gone" />

            <TextView
                android:id="@+id/client_activation_error_bs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:drawableTop="@drawable/ic_cross_grey"
                android:drawablePadding="10dp"
                tools:text="Unable to load your stuff" />

        </FrameLayout>
    </RelativeLayout>

    <TextView
        android:id="@+id/learning_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="@dimen/size_24"
        android:fontFamily="@font/lato_bold"
        android:letterSpacing="0"
        android:text="@string/learning"
        android:textColor="#080808"
        android:textSize="16sp" />

    <FrameLayout
        android:id="@+id/learning_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/size_16"
        android:layout_marginTop="16dp">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/learning_rv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="15dp"
            android:clipToPadding="false"
            android:nestedScrollingEnabled="false"
            android:paddingRight="20dp"
            android:visibility="gone" />

        <ProgressBar
            android:id="@+id/learning_progress_bar"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:visibility="gone" />

        <TextView
            android:id="@+id/learning_learning_error"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:drawableTop="@drawable/ic_cross_grey"
            android:drawablePadding="10dp"
            tools:text="Unable to load your stuff" />

    </FrameLayout>

    <RelativeLayout
        android:id="@+id/explore_by_role_rl"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView114"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="22dp"
            android:fontFamily="@font/lato"
            android:letterSpacing="0"
            android:text="@string/explore_by_role_"
            android:textColor="#080808"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:id="@+id/ll_search_role"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/cv_role"
            android:layout_alignBottom="@+id/cv_role"
            android:layout_alignParentEnd="true"
            android:layout_toEndOf="@+id/cv_role"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingStart="@dimen/size_41"
            android:paddingEnd="@dimen/size_24">


            <ImageView
                android:id="@+id/iv"
                android:layout_width="@dimen/size_24"
                android:layout_height="@dimen/size_24"
                app:srcCompat="@drawable/ic_search_lipstick" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="@string/see_more_underline"
                android:textColor="@color/lipstick"
                android:textSize="@dimen/font_14" />
        </LinearLayout>


        <androidx.cardview.widget.CardView
            android:id="@+id/cv_role"
            android:layout_width="@dimen/size_237"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView114"
            android:layout_alignParentStart="true"
            android:layout_marginTop="@dimen/size_16"
            android:layout_marginBottom="@dimen/size_16"
            android:minWidth="@dimen/size_144"

            app:cardCornerRadius="4dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"

                android:layout_height="wrap_content">


                <ImageView
                    android:id="@+id/iv_role"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/size_144"
                    android:adjustViewBounds="true"
                    android:scaleType="centerCrop"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:paddingTop="@dimen/size_9"
                    android:paddingBottom="@dimen/size_9"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/iv_role">

                    <TextView
                        android:id="@+id/tv_title_role"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:layout_marginBottom="8dp"
                        android:fontFamily="@font/lato"
                        android:letterSpacing="0"
                        android:textColor="#d9000000"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:text="Driver" />

                    <TextView
                        android:id="@+id/tv_subtitle_role"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:textColor="#000000"
                        android:textSize="10sp"
                        android:visibility="gone"
                        app:layout_constraintStart_toStartOf="@+id/tv_title_role"
                        app:layout_constraintTop_toBottomOf="@+id/tv_title_role"
                        tools:text="Lorem Ipsum is simply dummy" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>
    </RelativeLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/exploreByIndustryLayout"
        android:layout_width="match_parent"
        android:layout_height="215dp"
        android:visibility="gone">

        <TextView
            android:id="@+id/industry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="@dimen/size_24"
            android:fontFamily="@font/lato"
            android:letterSpacing="0"
            android:text="@string/explore_by_industry"
            android:textColor="#080808"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/explore_by_industry"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:clipToPadding="false"
            android:nestedScrollingEnabled="false"
            android:paddingRight="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/industry" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <TextView
        android:id="@+id/assessment_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="@dimen/size_24"
        android:fontFamily="@font/lato_bold"

        android:letterSpacing="0"
        android:text="@string/assessment_"
        android:textColor="#686868"
        android:textSize="16sp"
        android:visibility="gone" />


    <FrameLayout
        android:id="@+id/assessment_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:minHeight="170dp"
        android:visibility="gone">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/assessment_rv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="15dp"
            android:clipToPadding="false"
            android:nestedScrollingEnabled="false"
            android:visibility="gone" />

        <ProgressBar
            android:id="@+id/learning_assessment_progress_bar"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:visibility="gone" />

        <TextView
            android:id="@+id/learning_assessment_error"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:drawableTop="@drawable/ic_cross_grey"
            android:drawablePadding="10dp"
            tools:text="Unable to load your stuff" />

    </FrameLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="169dp">

        <View
            android:id="@+id/textView124"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:background="@color/shadow_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/help_topic_bs_calendar_screen"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView124" />

        <ImageView
            android:id="@+id/textView125"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
              app:srcCompat="@drawable/ic_browse_topic"
            app:layout_constraintLeft_toLeftOf="@+id/help_topic_bs_calendar_screen"
            app:layout_constraintTop_toTopOf="@+id/help_topic_bs_calendar_screen" />

        <TextView
            android:id="@+id/textView126"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="24dp"
            android:letterSpacing="0"
            android:text="@string/browse_all_help_topic"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView125"
            app:layout_constraintStart_toEndOf="@+id/textView125"
            app:layout_constraintTop_toTopOf="@+id/textView125" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="24dp"
              app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24"
            app:layout_constraintBottom_toBottomOf="@+id/help_topic_bs_calendar_screen"
            app:layout_constraintEnd_toEndOf="@+id/help_topic_bs_calendar_screen"
            app:layout_constraintTop_toTopOf="@+id/help_topic_bs_calendar_screen" />

        <View
            android:id="@+id/textView127"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_marginLeft="20dp"
            android:visibility="gone"
            android:background="@color/shadow_color"
            app:layout_constraintStart_toEndOf="@+id/textView125"
            app:layout_constraintTop_toBottomOf="@+id/help_topic_bs_calendar_screen" />

        <View
            android:id="@+id/contact_us_bs_calendar_screen"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:clickable="true"
            android:focusable="true"
            android:visibility="gone"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView127" />


        <ImageView
            android:id="@+id/contact_icon_bs_calendar_screen"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:visibility="gone"
              app:srcCompat="@drawable/ic_contact_us"
            app:layout_constraintLeft_toLeftOf="@+id/contact_us_bs_calendar_screen"
            app:layout_constraintTop_toTopOf="@+id/contact_us_bs_calendar_screen" />

        <TextView
            android:id="@+id/contact_us_txt_bs_calendar_screen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="24dp"
            android:letterSpacing="0"
            android:text="@string/contact_us"
            android:textColor="#000000"
            android:textSize="14sp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/contact_icon_bs_calendar_screen"
            app:layout_constraintStart_toEndOf="@+id/contact_icon_bs_calendar_screen"
            app:layout_constraintTop_toTopOf="@+id/contact_icon_bs_calendar_screen" />

        <ImageView
            android:id="@+id/right_arrow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="24dp"
              app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24"
            app:layout_constraintBottom_toBottomOf="@+id/contact_us_bs_calendar_screen"
            app:layout_constraintEnd_toEndOf="@+id/contact_us_bs_calendar_screen"
            app:layout_constraintTop_toTopOf="@+id/contact_us_bs_calendar_screen" />

        <View
            android:id="@+id/grayline2"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_marginLeft="20dp"
            android:background="@color/shadow_color"
            app:layout_constraintStart_toEndOf="@+id/contact_icon_bs_calendar_screen"
            app:layout_constraintTop_toBottomOf="@+id/contact_us_bs_calendar_screen" />

        <View
            android:id="@+id/invite_contact_bs_calendar_screen"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:clickable="true"
            android:focusable="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/grayline2" />

        <ImageView
            android:id="@+id/invite_icon_bs_calendar_screen"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="16dp"
